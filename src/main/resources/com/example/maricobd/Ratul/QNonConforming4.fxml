<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.maricobd.Ratul.QNonConformingController4">

    <children>
        <Label layoutX="160" layoutY="20" style="-fx-font-size: 18;" text="Non-Conforming Product Management" />

        <Label layoutX="50" layoutY="70" text="Batch Number:" />
        <TextField fx:id="batchTF" layoutX="180" layoutY="65" prefWidth="350" />

        <Label layoutX="50" layoutY="110" text="Defect Type:" />
        <TextField fx:id="defectTF" layoutX="180" layoutY="105" prefWidth="350" />

        <Label layoutX="50" layoutY="150" text="Quantity:" />
        <TextField fx:id="quantityTF" layoutX="180" layoutY="145" prefWidth="350" />

        <Label layoutX="50" layoutY="190" text="Corrective Action:" />
        <TextField fx:id="actionTF" layoutX="180" layoutY="185" prefWidth="350" />

        <Button defaultButton="true" layoutX="180" layoutY="240" onAction="#saveOnAction" prefHeight="27.0" prefWidth="66.0" text="Save" />
        <Button layoutX="499.0" layoutY="341.0" onAction="#logoutOnAction" prefHeight="27.0" prefWidth="58.0" text="Logout" />

        <Label fx:id="statusLabel" layoutX="180" layoutY="290" text="" />
      <Button defaultButton="true" layoutX="49.0" layoutY="21.0" mnemonicParsing="false" onAction="#backonAction" text="Back" />
    </children>
</AnchorPane>
